# Wedding Countdown App Configuration
# Copy this file to .env and fill in your values

# VAPID Keys for Web Push Notifications
# These will be auto-generated if not provided
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# Server Configuration
PORT=3000

# Email Configuration for Daily Notifications
# For Gmail, you need to use an App Password: https://support.google.com/accounts/answer/185833
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
